{
  "expo": {
    "name": "mon-app-frontend",
    "slug": "mon-app-frontend",
    "version": "1.0.0",
    "owner": "egawilldoit1",
    "orientation": "portrait",

    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",

    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#F3F6FA",
          "image": "./assets/splash-logo.png",
          "resizeMode": "contain",
          "imageWidth": 360,                       
          "dark": {
            "backgroundColor": "#0E1116",
            "image": "./assets/splash-logo.png",
            "resizeMode": "contain",
            "imageWidth": 280
          }
        }
      ]
    ],

    "splash": {
      "image": "./assets/splash-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#F3F6FA"
    },

    "assetBundlePatterns": ["**/*"],

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.egawilldoit.monappfrontend"
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.egawilldoit.monappfrontend"
    },

    "web": { "favicon": "./assets/favicon.png" },

    "extra": {}
  }
}
